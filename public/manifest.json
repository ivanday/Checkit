{
  "name": "Checkit",
  "description": "Check if the website you are currently visiting has been posted to Reddit",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "icons": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
  },
  "permissions": [
    "tabs"
  ]
}